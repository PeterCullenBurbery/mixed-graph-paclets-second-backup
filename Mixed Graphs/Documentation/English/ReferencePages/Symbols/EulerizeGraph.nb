Cell[BoxData[
 InterpretationBox[GridBox[{
    {
     RowBox[{
      RowBox[{"EulerizeGraph", "[", 
       RowBox[{
       "PeterBurbery`MixedGraphs`Private`graph_", "?", "ConnectedGraphQ"}], 
       "]"}], ":=", 
      RowBox[{"NestWhile", "[", 
       RowBox[{
        RowBox[{"Function", "[", 
         RowBox[{"PeterBurbery`MixedGraphs`Private`x", ",", 
          RowBox[{"EdgeAdd", "[", 
           RowBox[{"PeterBurbery`MixedGraphs`Private`x", ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"First", "[", "#1", "]"}], "\[UndirectedEdge]", 
                RowBox[{"Last", "[", "#1", "]"}]}], "&"}], ")"}], "[", 
             RowBox[{"First", "[", 
              RowBox[{"TakeDrop", "[", 
               RowBox[{
                RowBox[{"Select", "[", 
                 RowBox[{
                  RowBox[{
                  "VertexList", "[", "PeterBurbery`MixedGraphs`Private`x", 
                   "]"}], ",", 
                  RowBox[{
                   RowBox[{"OddQ", "[", 
                    RowBox[{"VertexDegree", "[", 
                    RowBox[{"PeterBurbery`MixedGraphs`Private`x", ",", "#1"}],
                     "]"}], "]"}], "&"}]}], "]"}], ",", "2"}], "]"}], "]"}], 
             "]"}]}], "]"}]}], "]"}], ",", 
        "PeterBurbery`MixedGraphs`Private`graph", ",", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"EulerianGraphQ", "[", "#1", "]"}]}], "&"}]}], "]"}]}]},
    {" "}
   },
   BaselinePosition->{Baseline, {1, 1}},
   GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}}],
  ColumnForm[{PeterBurbery`MixedGraphs`EulerizeGraph[
      PatternTest[
       Pattern[PeterBurbery`MixedGraphs`Private`graph, 
        Blank[]], ConnectedGraphQ]] := NestWhile[
      Function[PeterBurbery`MixedGraphs`Private`x, 
       EdgeAdd[PeterBurbery`MixedGraphs`Private`x, 
        (UndirectedEdge[
         First[#], 
         Last[#]]& )[
         First[
          TakeDrop[
           Select[
            VertexList[PeterBurbery`MixedGraphs`Private`x], OddQ[
             VertexDegree[PeterBurbery`MixedGraphs`Private`x, #]]& ], 2]]]]], 
      PeterBurbery`MixedGraphs`Private`graph, Not[
       EulerianGraphQ[#]]& ], 
    SpaceForm[1]}],
  Editable->False]], "Output",
 CellChangeTimes->{3.8671614393096175`*^9},
 CellLabel->"Out[4]=",
 CellID->266615844,ExpressionUUID->"25893d45-3421-45d6-81fa-dba8c0c7262c"]
}, Open  ]],

